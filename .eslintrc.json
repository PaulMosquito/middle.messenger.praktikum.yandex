{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base",
		"airbnb-typescript/base"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-underscore-dangle": 0,
		"import/no-cycle": 0,
		"no-unused-vars": 0,
		"class-methods-use-this": 0,
		"no-param-reassign": 0,
		"@typescript-eslint/no-unused-vars": 0,
		"indent": ["error", 4],
		"@typescript-eslint/indent": ["error", 4]
	}
}